    <section class="tab-section" id="tab-section-6">

        <div class="tab">
            <button class="tablinks" onclick="openTab(event, 'austria-vienna-hofburg-palace-complex', 'tab-section-6')">Hofburg Palace &#9662</button>
            <button class="tablinks" onclick="openTab(event, 'austria-vienna-churches-and-museums', 'tab-section-6')">Churches &amp; Museums &#9662</button>
