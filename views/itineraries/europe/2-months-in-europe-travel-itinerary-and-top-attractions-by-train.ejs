    <section class="tab-section" id="tab-section-15">

        <!-- Tab Link Buttons -->
        <div class="tab">
            <button class="tablinks" onclick="openTab(event, 'belgium-brussels-central-brussels', 'tab-section-15')">Central Brussels &#9662</button>
