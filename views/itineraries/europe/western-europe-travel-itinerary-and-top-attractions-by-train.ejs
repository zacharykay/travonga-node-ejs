        <!-- Tab Link Buttons -->
        <div class="tab">
            <button class="tablinks" onclick="openTab(event, 'belgium-brussels-central-brussels', 'tab-section-7')">Central Brussels &#9662</button>
            <button class="tablinks" onclick="openTab(event, 'belgium-brussels-more-top-attractions', 'tab-section-7')">More Top Attractions &#9662</button>
