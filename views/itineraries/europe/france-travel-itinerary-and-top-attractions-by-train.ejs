    <section class="tab-section" id="tab-section-4">

        <!-- Tab Link Buttons -->
        <div class="tab">
            <button class="tablinks" onclick="openTab(event, 'france-strasbourg-iconic-sights', 'tab-section-4')">Iconic Sights &#9662</button>
